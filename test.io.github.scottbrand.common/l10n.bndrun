-runfw: org.apache.felix.framework;version='[5.6.2,5.6.2]'
-runee: JavaSE-1.8

-runvm: \
	-Djava.io.tmpdir=tmp,\
	-Dorg.eclipse.jetty.util.log.class=org.eclipse.jetty.util.log.StdErrLog,\
    "-Dfelix.fileinstall.dir=etc",\
	"-Dfelix.fileinstall.tmpdir=tmp",\
	"-Dfelix.fileinstall.noInitialDelay=true",\
	 -Dosgi.requiredJavaVersion=1.8,\
	-Xms256m,\
	-Xmx1024m



-runbundles: \
	felix-run,\
	osgi-run,\
	io.github.scottbrand.common;version=latest,\
	io.github.scottbrand.common.provider.l10n;version=latest,\
	test.io.github.scottbrand.common.l10n;version=latest